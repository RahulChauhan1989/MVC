                {
                    if (Session["ClientPackage"] != null && (Convert.ToByte(Session["ClientPackage"]) == 1))
                    {
                        <li class = "tabs1" id="@ViewBag.PackageCssClass">@Html.ActionLink("Manage Packages", "ManagePackages", "Client", new { CId = ViewBag.ClientID }, null)</li>

<div class="row">
    <div class="col-md-12">
        <ul id="tabs">
            @{
                if (Session["ClientContract"] != null && (Convert.ToByte(Session["ClientContract"]) == 1))
                {
                    <li class = "tabs1" id="@ViewBag.ContractCssClass">@Html.ActionLink("Upload Contract", "ManageClientContract", "Client", new { CId = ViewBag.ClientID }, null)</li>
                }
                else
                {
                    <li id="@ViewBag.ContractCssClass">@Html.ActionLink("Upload Contract", "ManageClientContract", "Client", new { CId = ViewBag.ClientID }, null)</li>
                }

                if (Session["PricingType"] != null && (Convert.ToByte(Session["PricingType"]) == 2 || Convert.ToByte(Session["PricingType"]) == 3))
                {
                    if (Session["ClientCheck"] != null && (Convert.ToByte(Session["ClientCheck"]) == 1))
                    {
                            <li class = "tabs1" id="@ViewBag.CheckCssClass">@Html.ActionLink("Manage Checks", "ManageClientChecks", "Client", new { CId = ViewBag.ClientID }, null)</li>
                    }
